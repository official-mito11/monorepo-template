generator client {
  provider = "prisma-client-js"
}

generator prismabox {
  provider = "prismabox"
  output   = "../../../packages/shared/generated/prisma"
}

datasource db {
  provider = "sqlite"
}

model User {
  id        String   @id @default(uuid())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  
  @@index([id])
  @@map("users")
}