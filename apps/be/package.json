{
  "name": "@app/be",
  "version": "0.1.0",
  "module": "src/index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "bun run dist/index.js",
    "dev": "bun run src/index.ts --hot",
    "build": "bun build src/index.ts --outdir dist --target bun",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "db:generate": "bunx prisma generate",
    "db:migrate": "bunx prisma migrate dev",
    "db:push": "bunx prisma db push",
    "db:studio": "bunx prisma studio",
    "db:seed": "bunx prisma db seed",
    "generate:spec": "bun scripts/generate-spec.ts",
    "generate:openapi": "bunx openapi-typescript-codegen --input ../../packages/api-client/openapi-spec.json --output ../../packages/api-client/generated --client axios",
    "postprocess:openapi": "bun scripts/postprocess-openapi.ts",
    "generate:client": "bun run generate:spec && bun run generate:openapi && bun run postprocess:openapi",
    "change-prisma": "bun scripts/change-prisma.ts"
  },
  "devDependencies": {
    "@prisma/generator-helper": "^7.2.0",
    "@types/bun": "latest",
    "@types/node": "^24.10.4",
    "dotenv": "^16.6.1",
    "openapi-typescript-codegen": "^0.29.0",
    "prisma": "^7.2.0"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@app/shared": "workspace:*",
    "@bogeychan/elysia-logger": "^0.1.10",
    "@elysiajs/cookie": "^0.8.0",
    "@elysiajs/cors": "^1.4.0",
    "@elysiajs/jwt": "^1.4.0",
    "@elysiajs/openapi": "^1.4.12",
    "@elysiajs/static": "^1.4.7",
    "@elysiajs/swagger": "^1.3.1",
    "@elysiajs/html": "^1.4.0",
    "@libsql/client": "^0.15.15",
    "@prisma/adapter-libsql": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@sinclair/typebox": "^0.34.45",
    "axios": "^1.13.2",
    "elysia": "^1.4.19",
    "elysia-rate-limit": "^4.4.2",
    "pino-pretty": "^13.1.3",
    "prismabox": "^1.1.26"
  }
}
